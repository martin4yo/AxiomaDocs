{
  "name": "axioma-docs-server",
  "version": "1.0.0",
  "description": "API server for document management system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "npx prisma generate && tsc",
    "start": "node dist/index.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:studio": "npx prisma studio",
    "db:seed": "ts-node scripts/seed.ts",
    "migrate-data": "ts-node migrate-mysql-to-postgres.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "archiver": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.15.1",
    "node-cron": "^3.0.3",
    "pdfkit": "^0.14.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.13.4",
    "prisma": "^5.7.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
